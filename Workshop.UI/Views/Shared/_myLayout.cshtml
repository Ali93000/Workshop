@using Workshop.UI.Resources
<!DOCTYPE html>
<html class="loading" lang="en" data-textdirection="rtl">
<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">

    <title>
        وزارة العدل - منصة نمذجة الاشعارات
    </title>
    <link rel="apple-touch-icon" href="~/Content/Template/app-assets/images/ico/apple-icon-120.png">
    <link rel="shortcut icon" type="image/x-icon" href="~/Content/Template/assets/demo/default/media/img/logo/favicon.ico">
    <link href="~/Content/Template/app-assets/fonts/line-awesome/css/line-awesome.min.css" rel="stylesheet" />

    <!-- BEGIN VENDOR CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/vendors.css">
    <!-- END VENDOR CSS-->
    <!-- BEGIN MODERN CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/app.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/custom-rtl.css">
    <!-- END MODERN CSS-->
    <!-- BEGIN Page Level CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/core/menu/menu-types/vertical-menu.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/core/colors/palette-gradient.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/vendors/css/cryptocoins/cryptocoins.css">

    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/core/colors/palette-switch.css">

    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/vendors/css/forms/toggle/bootstrap-switch.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/vendors/css/forms/toggle/switchery.min.css">

    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/css-rtl/plugins/forms/switch.css">
    <!-- END Page Level CSS-->
    <!-- BEGIN Custom CSS-->
    <link rel="stylesheet" type="text/css" href="~/Content/Template/assets/css/style-rtl.css">

    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/vendors/css/forms/selects/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/vendors/css/tables/datatable/datatables.min.css">

    <link rel="stylesheet" type="text/css" href="~/Content/Template/app-assets/vendors/css/forms/icheck/icheck.css">

    <!--begin::Page Vendors -->
    <link href="~/Content/Template/assets/vendors/custom/fullcalendar/fullcalendar.bundle.css" rel="stylesheet" type="text/css" />
    <!--RTL version:<link href="assets/vendors/custom/fullcalendar/fullcalendar.bundle.rtl.css" rel="stylesheet" type="text/css" />-->
    <!--end::Page Vendors -->
    <!--		<link href="assets/vendors/base/vendors.bundle.css" rel="stylesheet" type="text/css" />-->
    <link href="~/Content/Template/assets/vendors/base/vendors.bundle.rtl.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Template/custom css/style.bundle.rtl.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Template/custom css/custom.css" rel="stylesheet" type="text/css" />
</head>

<body class="m-page--fluid m--skin- m-content--skin-light2 m-header--fixed m-header--fixed-mobile m-aside-left--enabled m-aside-left--skin-light m-aside-left--fixed m-aside-left--offcanvas m-footer--push m-aside--offcanvas-default vertical-layout vertical-menu 2-columns   menu-expanded fixed-navbar"
      data-open="click" data-menu="vertical-menu" data-col="2-columns">
    <!-- fixed-top-->
    <!-- ////////////////////////////////////////////////////////////////////////////-->
    <!-- begin::Body -->

    <div class="m-grid m-grid--hor m-grid--root m-page">
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-body">
            
            <div id="m_aside_left" class="m-grid__item	m-aside-left  m-aside-left--skin-light " @*style="background: url(/images/945396.jpg) no-repeat center 0;"*@>

                <!-- BEGIN: Aside Menu -->
                <div id="m_ver_menu" class="m-aside-menu  m-aside-menu--skin-light m-aside-menu--submenu-skin-light " m-menu-vertical="1" m-menu-scrollable="1" m-menu-dropdown-timeout="500" style="position: relative;">
                    <ul class="m-menu__nav  m-menu__nav--dropdown-submenu-arrow ">
                        <li class="m-menu__item  m-menu__item--active" aria-haspopup="true">
                            <a href="@Url.Action("Index", "Home")" class="m-menu__link">
                                <i class="m-menu__link-icon flaticon-line-graph"></i>
                                <span class="m-menu__link-title">
                                    <span class="m-menu__link-wrap">
                                        <span class="m-menu__link-text" style="color:black">@SysResource.Menu_Main</span>
                                    </span>
                                </span>
                            </a>
                        </li>
                        
                        <li class="m-menu__item  m-menu__item--submenu" aria-haspopup="true" m-menu-submenu-toggle="hover">
                            <a href="javascript:;" class="m-menu__link m-menu__toggle">
                                <i class="m-menu__link-icon flaticon-avatar"></i>
                                <span class="m-menu__link-text" style="color:black">@SysResource.Menu_Customers</span>
                                <i class="m-menu__ver-arrow la la-angle-left"></i>
                            </a>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="@Url.Action("Index", "Customer")" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet--dot">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text"> @SysResource.MenuSub_Customers </span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                            <div class="m-menu__submenu ">
                                <span class="m-menu__arrow"></span>
                                <ul class="m-menu__subnav">
                                    <li class="m-menu__item " aria-haspopup="true">
                                        <a href="@Url.Action("Create", "Customer")" class="m-menu__link ">
                                            <i class="m-menu__link-bullet m-menu__link-bullet--dot">
                                                <span></span>
                                            </i>
                                            <span class="m-menu__link-text"> @SysResource.CustomerCreate</span>
                                        </a>
                                    </li>
                                </ul>
                            </div>
                        </li>

                        <!-- END: Aside Menu -->
                </div>
            </div>

            <!-- BEGIN: Left Aside -->
            @{ Html.RenderPartial("_Header"); }

            <!-- END: Left Aside -->

            <div class="m-grid__item m-grid__item--fluid m-wrapper">
                <div class="m-content">
                    @RenderBody()
                </div>
            </div>
        </div>
        <!-- end:: Body -->
        @*@{Html.RenderPartial("_VHeaderUsers");}*@
        <footer class="m-grid__item		m-footer ">
            <div class="m-container m-container--fluid m-container--full-height m-page__container">
                <div class="m-stack m-stack--flex-tablet-and-mobile m-stack--ver m-stack--desktop">
                    <div class="m-stack__item m-stack__item--left m-stack__item--middle m-stack__item--last">
                        <span class="m-footer__copyright">
                            جميع الحقوق محفوظة لدى
                            <a href="" class="m-link">وزارة العدل</a>
                            2019 &copy;
                        </span>
                    </div>

                </div>
            </div>
        </footer>
    </div>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>

    <script src="~/Content/Template/app-assets/vendors/js/forms/spinner/jquery.bootstrap-touchspin.js"
            type="text/javascript"></script>
    <script src="~/Content/Template/app-assets/vendors/js/forms/validation/jqBootstrapValidation.js"
            type="text/javascript"></script>
    <script src="~/Content/Template/app-assets/vendors/js/forms/toggle/bootstrap-switch.min.js"
            type="text/javascript"></script>
    <script src="~/Content/Template/app-assets/vendors/js/forms/toggle/switchery.min.js" type="text/javascript"></script>

    <script src="~/Content/Template/app-assets/vendors/js/forms/select/select2.full.min.js" type="text/javascript"></script>
    <script src="~/Content/Template/app-assets/js/scripts/forms/select/form-select2.js" type="text/javascript"></script>

    <script src="~/Content/Template/app-assets/vendors/js/charts/chart.min.js" type="text/javascript"></script>

    <script src="~/Content/Template/app-assets/vendors/js/forms/icheck/icheck.min.js" type="text/javascript"></script>
    <script src="~/Content/Template/app-assets/vendors/js/forms/toggle/bootstrap-checkbox.min.js"
            type="text/javascript"></script>

    <script src="~/Content/Template/app-assets/vendors/js/charts/jquery.sparkline.min.js" type="text/javascript"></script>
  
    <script src="~/Content/Template/app-assets/js/scripts/tables/components/table-components.js"
            type="text/javascript"></script>
 
    @*For Table*@
    <script src="~/Content/Template/app-assets/vendors/js/tables/jquery.dataTables.min.js"></script>
    <link href="~/Content/Template/app-assets/vendors/css/tables/datatable/jquery.dataTables.min.css" rel="stylesheet" />

    <script>
        //Search Table
        $("#loginUsername").text("مرحبا : " + localStorage.getItem("Username"));
    </script>
    <script src="~/Content/Template/assets/vendors/base/vendors.bundle.js"></script>
    <script src="~/Content/Template/assets/demo/default/base/scripts.bundle.js"></script>
    <script src="~/Content/Template/assets/vendors/custom/datatables/datatables.bundle.js"></script>
    <!--end::Page Vendors -->
    <!--begin::Page Resources -->
    <script src="~/Content/Template/assets/demo/default/custom/crud/datatables/basic/paginations.js"></script>
    <!--end::Base Scripts -->
    <!--begin::Page Vendors -->
    <script src="~/Content/Template/assets/vendors/custom/fullcalendar/fullcalendar.bundle.js"></script>
    <!--end::Page Vendors -->
    <!--begin::Page Snippets -->
    <script src="~/Content/Template/custom css/dashboard.js"></script>
</body>
</html>